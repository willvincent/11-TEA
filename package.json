{
  "name": "11-TEA",
  "description": "11ty, TailwindCSS 3, AlpineJS 3 starter",
  "author": "Will Vincent <will@willvincent.com> (https://willvincent.com)",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev:css": "npx tailwindcss -i src/style.css -o dist/style.css -w",
    "dev:11ty": "eleventy --serve",
    "dev": "node_modules/.bin/run-p dev:*",
    "build:css": "NODE_ENV=production npm tailwindcss -i src/style.css -o dist/style.css -m",
    "build:11ty": "eleventy",
    "build": "node_modules/.bin/run-s build:*"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/typography": "^0.5.0",
    "html-minifier": "^4.0.0",
    "js-yaml": "^4.1.0",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.0.2"
  }
}
